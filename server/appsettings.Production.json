{
    "Logging": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft.AspNetCore": "Warning"
      }
    },
    "AllowedHosts": "*",
    "ConnectionStrings": {
      "DefaultConnection": "${ConnectionStrings__DefaultConnection}"
    },
    "Jwt": {
      "Key": "${Jwt__Key}",
      "Issuer": "${Jwt__Issuer}",
      "Audience": "${Jwt__Audience}"
    }
  }